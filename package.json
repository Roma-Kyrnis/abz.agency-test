{
  "name": "news-of-the-packages-backend",
  "version": "0.0.1",
  "description": "Read package updates",
  "main": "src/main.ts",
  "packageManager": "pnpm@8.10.5+sha256.a4bd9bb7b48214bbfcd95f264bd75bb70d100e5d4b58808f5cd6ab40c6ac21c5",
  "license": "ISC",
  "author": "Roman Kyrnis",
  "homepage": "https://github.com/Roma-Kyrnis/news-of-the-packages-backend#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Roma-Kyrnis/news-of-the-packages-backend.git"
  },
  "bugs": {
    "url": "https://github.com/Roma-Kyrnis/news-of-the-packages-backend/issues"
  },
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start -b swc",
    "start:dev": "NODE_ENV=development nest start --builder swc --watch",
    "start:debug": "NODE_ENV=development nest start --debug --watch",
    "start:prod": "NODE_ENV=production node dist/main",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:run": "typeorm-ts-node-commonjs migration:run -d src/database/datasource.config.ts",
    "migration:revert": "typeorm-ts-node-commonjs migration:revert -d src/database/datasource.config.ts",
    "migration:show": "typeorm-ts-node-commonjs migration:show -d src/database/datasource.config.ts",
    "seed": "NODE_ENV=development ts-node src/database/seeder/seeder.ts",
    "seed:prod": "node dist/database/seeder/seeder.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.2.1",
    "@nestjs/schematics": "^10.0.3",
    "@nestjs/testing": "^10.2.9",
    "@swc/cli": "^0.1.63",
    "@swc/core": "^1.3.96",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.9.2",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "prettier": "^3.1.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.2"
  },
  "dependencies": {
    "@google-cloud/storage": "^7.6.0",
    "@nestjs/common": "^10.2.9",
    "@nestjs/core": "^10.2.9",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/platform-express": "^10.2.9",
    "@nestjs/typeorm": "^10.0.1",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "helmet": "^7.1.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.6.4",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.17",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  }
}
